/**app.wxss**/
@import "/lib/style/stylesheet.wxss";
Page
{
  background: white;
}

.corner-icon::before {
  margin: 0;
  top: -10rpx;
  display: inline-block;
  position: absolute;
  padding: 0;
  border: 0;
  color: red;
  vertical-align: baseline;
  font: normal normal normal 14px/1 vant-icon-76f274;
  font-size: 64rpx;
  z-index: 5;
  content: "\F034";
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

  .container {
  padding: 25rpx;
}  

/* flex布局 通用 */

 .flex {
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  text-align: center;
} 

.flexs {
  display: flex;
  flex-direction: row;
  /* align-items: center; */
  box-sizing: border-box;
  text-align: center;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.active {
  color: #fec321;
}
.noPage{
   text-align: center;
   display: block;
   width: 100%;
   clear: both;
}
.noPage image{
  width: 150rpx;
  height: 50rpx;
  margin-top: 35rpx;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.fixLayout
{
  display: flex;
}


.fixButton
{
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  right:5rpx;
  line-height: 120rpx;
}

.firstButton
{
  bottom: 10rpx;
}

.firstButtonNormal
{
  bottom: 100rpx;
}

.secondButton
{
  bottom:110rpx;
}

.topSecondButton
{
  bottom:400rpx;
}

.topFirstButton
{
  top: 30rpx;
}

.secondButtonNormal
{
  bottom:200rpx;
}
 

.fixButton image
{
  width: 100%;
  height: 100%;
}

.gradientBackground
{
  background: linear-gradient(to right, #ec3721, #ff7442);
  color:white;
}

.reverseGradientBackground
{
  background: linear-gradient(to right,#ff7442, #ec3721);
  color:white;
}

.mainGradientBackground
{
  background: #ec3721;
  color:white;
}

.disabled
{
  color: #999;
  background: #f5f5f5;
}

.noPage{
  position: relative;
  width: 100%;
}
.noPage image{
  position: relative;
  width: 150rpx;
  height: 50rpx;
  left: 50%;
  transform: translateX(-50%);
}

.bold
{
  font-weight: bold;
}

button::after{
border:none;
background: none  ;
}

input{
outline:none;
border:none;
list-style: none;
background: none  ;
}
