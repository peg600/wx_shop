/* pages/toPersenalShop/toPersonalShop.wxss */
page{
  position: relative;
  box-sizing: border-box;
  min-height: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

view{
  box-sizing: border-box;
}

.shop-info-wrapper{
  position: relative;
  width: 100%;
  padding: 32rpx 40rpx 0 40rpx;
  border-top: 2rpx solid rgb(229, 229, 229);
}

.shop-info-content{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 340rpx;
}

.shop-info-bac{
  position: absolute;
  width:100%;
  height: 340rpx;
}

.shop-info{
  position: relative;
  width: 100%;
  height: 100%;
  padding: 0 20rpx;
}

.top{
  display: flex;
  position: relative;
  width: 100%;
  height: 240rpx;
  padding: 35rpx 17rpx;
  overflow: hidden;
}

.top-left{
  position: relative;
  height: 100%;
  flex: 0 0 184rpx;
}
.top-left image{
  position: relative;
  width: 100%;
  height: 100%;
}

.personal_shop_thumb{
  position: absolute;
  width: 100%;
  height: 100%;
  padding: 15rpx;
  z-index: 1;
}
.personal_shop_thumb image{
  position: relative;
  width: 100%;
  height: 100%;
}

.top-right{
  position: relative;
  height: 100%;
  flex: 1 1;
  padding: 25rpx 0 36rpx 52rpx;
}

.shop-name{
  position: relative;
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 36rpx;
  color: rgb(64, 64, 64);
}

.shop-btn{
  position: relative;
  width: 100%;
  height: 58rpx;
  top: 35rpx;
}

.enter{
  display: inline-block;
  position: relative;
  width: 211rpx;
  height: 58rpx;
  vertical-align: middle;
  border-radius: 29rpx;
  text-align: center;
  line-height: 58rpx;
  font-size: 24rpx;
  color: white;
  background-color: rgb(255, 192, 0);
}

.share{
  display: inline-block;
  position: relative;
  width: 50rpx;
  height: 50rpx;
  margin-left: 40rpx;
  vertical-align: middle;
  text-align: center;
  line-height: 50rpx;
  border-radius: 50%;
  background-color: rgb(255, 177, 47);
}
.share image{
  position: relative;
  width: 100%;
  height: 100%;
}


.bottom{
  display: flex;
  position: relative;
  width: 100%;
  height: 112rpx;
  padding: 6rpx 0 13rpx 6rpx;
  border-top: 2rpx dotted rgb(49, 49, 49);
}

.bottom-left{
  position: relative;
  height: 84rpx;
  flex: 0 0 150rpx;
  margin-left: 10rpx;
  margin-right: 5rpx;
  line-height: 84rpx;
  font-size: 30rpx;
  color: black;
}

.bottom-right{
  position: relative;
  height: 84rpx;
  flex: 1 1 ;
}

.history-list{
  display: inline-block;
  position: relative;
  height: 84rpx;
  line-height: 84rpx;
  font-size: 24rpx;
  color: rgb(117, 117, 117);
}

.history{
  display: inline-block;
  position: relative;
  width: 84rpx;
  height: 84rpx;
  margin-left: 15rpx;
  vertical-align: middle;
  border-radius: 50%;
  overflow: hidden;
}
.history image{
  position: relative;
  width: 100%;
  height: 100%;
}

.no-history{
  display: inline-block;
  position: relative;
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  font-size: 24rpx;
  color: rgb(117, 117, 117);
}

.more{
  display: inline-block;
  position: relative;
  width: 84rpx;
  height: 84rpx;
  margin-left: 15rpx;
  border-radius: 50%;
  vertical-align: middle;
  text-align: center;
  overflow: hidden;
  background-color: white;
}

.dots{
  display: inline-block;
  position: relative;
  height: 10rpx;
  
}

.dot{
  display: inline-block;
  position: relative;
  width: 10rpx;
  height: 10rpx;
  margin-right: 6rpx;
  border-radius: 50%;
  background-color: rgb(31, 31, 31);
}

.shop-list{
  position: relative;
  width: 100%;
  margin-top: 36rpx;
  padding: 0 40rpx 34rpx 40rpx;
  text-align: center;
}

.list-title{
  position: relative;
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: left;
  font-size: 30rpx;
  color: rgb(26, 26, 26);
}

.more-list{
  display: flex;
  position: relative;
  width: 120rpx;
  height: 40rpx;
  padding: 10rpx 0;
  line-height: 20rpx;
  float: right;
  text-align: right;
  font-size: 20rpx;
  color: rgb(94, 94, 94);
}
.more-list view{
  position: relative;
  height: 100%;
  flex: 1 1 ;
  text-align: right;
}
.more-list image{
  position: relative;
  flex: 0 0 10rpx;
  height: 100%;
  margin-left: 10rpx;
}

.list{
  display: inline-block;
  position: relative;
  max-width: 672rpx;
  margin-top: 22rpx;
  text-align: left;
}

.shop-item{
  display: inline-block;
  position: relative;
  width: 325rpx;
  height: 410rpx;
  margin-bottom: 35rpx;
}
.shop-item:nth-child(odd){
  margin-right: 22rpx;
}

.shop-thumb{
  position: relative;
  width: 325rpx;
  height: 325rpx;
  border-radius: 20rpx;
}

.list-shop-name{
  position: relative;
  height: 28rpx;
  margin-top: 16rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  color: rgb(26, 26, 26);
}

.list_shop_slogan{
  position: relative;
  height: 24rpx;
  margin-top: 16rpx;
  line-height: 24rpx;
  font-size: 28rpx;
  color: rgb(94, 94, 94);
}




