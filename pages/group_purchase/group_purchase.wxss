.jump-wrapper{
  display: flex;
  position: relative;
  height: 100%;
  width: 100%;
}

.goods-name{
  position: relative;
  height: 100%;
  flex: 1 1;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  /* white-space: nowrap; */
}

.jump-icon{
  position: relative;
  flex: 0 0 22rpx;
  line-height: 100%;
}
.jump-icon image{
  display: inline-block;
  position: relative;
  width: 100%;
  height: 39rpx;
  top: 50%;
  transform: translateY(-50%);
}

.failed{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  margin-bottom: 20rpx;
  text-align: center;
}

.time-count{
  margin-top: 18rpx;
  font-size: 24rpx;
}

.join{
  display: inline-block;
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 84rpx;
  margin-top: 24rpx;
  margin-bottom: 16rpx;
  padding: 0 54rpx;
  text-align: center;
  border-radius: 10rpx;
  line-height: 84rpx;
  font-size: 28rpx;
  color: white;
}
.join view{
  position: relative;
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  background-color: red;  
}

.group-list{
  position: relative;
  width: 600rpx;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 20rpx;
  text-align: center;
  border-radius: 10rpx;
  font-size: 22rpx;
  color: rgb(230, 0, 18);
  background-color: pink;
}

.group-item-list{
  position: relative;
  width: 100%;
  padding: 22rpx 30rpx; 
}

.group-item{
  position: relative;
  width: 100%;
  height: 62rpx;
  margin-top: 22rpx;
  color: black;
}

.group-left{
  display: flex;
  position: relative;
  height: 100%;
  max-width: 200rpx;
  float: left;
  font-size: 20rpx;
}
.group-left image{
  position: relative;
  height: 100%;
  flex: 0 0 62rpx;
  border-radius: 50%;
}
.group-left text{
  position: relative;
  height: 100%;
  flex: 1 1;
  padding-left: 10rpx;
  line-height: 62rpx;
  text-align: left;

}

.group-right{
  display: flex;
  position: relative;
  width: 250rpx;
  height: 40rpx;
  top: 50%;
  transform: translateY(-50%);
  float: right;
}

.group-info{
  position: relative;
  height: 100%;
  flex: 1 1;
  margin-right: 10rpx;
  text-align: right;
}

.group-join{
  position: relative;
  height: 100%;
  flex: 0 0 100rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 40rpx;
  color: white;
  font-size: 20rpx;
  background-color: rgb(230, 0, 18);
}

.need-number{
  position: relative;
  height: 20rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  white-space: nowrap;
}
.need-number text{
  color: rgb(230, 0, 18);
}

.group-time-count{
  position: relative;
  height: 20rpx;
  font-size: 14rpx;
  line-height: 20rpx;
}

.title{
  position: relative;
  width: 100%;
  height: 100rpx;
  padding: 0 50rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #333;
}

.rules{
  position: relative;
  width: 100%;
  margin-bottom: 50rpx;
  padding: 0 50rpx;
}
.rules view{
  width: 100%;
  line-height: 50rpx;
  font-size: 30rpx;
  color: red;
}

.mask{
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 100;
}

.share{
  position: relative;
  width: 600rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  padding: 30rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: white;
}
.share view{
  width: 100%;
  line-height: 100rpx;
  font-size: 34rpx;
  color: red;
}
.share button{
  width: 400rpx;
  height: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 50rpx;
  border-radius: 15rpx;
  font-size: 30rpx;
  color: white;
  background-color: rgb(0, 186, 9);
}

.inviter{
  display: flex;
  align-items: center;
  position: relative;
  width: 100%;
  padding: 20rpx;
  margin-bottom: 10rpx;
}
.inviter image{
  position: relative;
  flex: 0 0 100rpx;
  height: 100rpx;
  margin-right: 30rpx;
  border-radius: 50%;
}
.inviter view{
  flex: 1 1;
  text-align: left;
  line-height: 100rpx;
  font-size: 40rpx;
  color: #333;
}

.gray{
  background-color: gray;
}

.coupon{
  display: flex;
  position: relative;
  width: 90%;
  height: 170rpx;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 50rpx;
  margin-bottom: 30rpx;
  background-color: rgb(240, 240 , 30);
}
.coupon:before, .coupon:after {
    content: ' ';
    width: 0;
    height: 100%;
    /* 绝对定位进行偏移 */
    position: absolute;
    top: 10rpx;
}

.coupon:before {
    /* 圆点型的border */
    border-right: 10rpx dotted white;
    /* 偏移一个半径，让圆点的一半覆盖div */
    left: -4rpx;
}

.coupon:after {
    /* 圆点型的border */
    border-left: 10rpx dotted white;
    /* 偏移一个半径，让圆点的一半覆盖div */
    right: -4rpx;
}

.coupon-left{
  position: relative;
  flex: 1 1;
  height: 100%;
  padding: 0 20rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  color: white;
}
.coupon-left view{
  position: relative;
  width: 100%;
  height: 100rpx;
  top: 50%;
  transform: translateY(-50%);
  text-align: center;
  line-height: 100rpx;
  font-size: 80rpx;
}

.coupon-right{
  position: relative;
  flex: 0 0 200rpx;
  height: 100%;
  line-height: 170rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}
.coupon-right:before {
    content: ' ';
    width: 0;
    height: 100%;
    /* 绝对定位进行偏移 */
    position: absolute;
    top: 0rpx;
    /* 圆点型的border */
    border-right: 10rpx dotted white;
    /* 偏移一个半径，让圆点的一半覆盖div */
    left: -4rpx;
}

.form-button{
  position: fixed;
  box-sizing: border-box;
  width: calc(100% - 120rpx);
}









