/* pages/group_purchase/group_purchase.wxss */
page{
  position: relative;
  box-sizing: border-box;
  min-height: 100%;
  padding: 0;
  margin: 0;
}

view{
  box-sizing: border-box;
}

.container{
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

.top{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding: 28rpx 34rpx 44rpx 34rpx;
  background-color: pink;
}

.goods-info-wrapper{
  display: flex;
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding: 0 0 80rpx 24rpx;
  border-radius: 24rpx;
  background-color: white;
}

.left{
  position: relative;
  height: 100%;
  flex: 0 0 430rpx;
  padding-top: 56rpx;
}

.right{
  position: relative;
  box-sizing: border-box;
  height: 100%;
  flex: 1 1;
  padding: 0 24rpx 20rpx 20rpx;
}

.pic-wrapper{
  position: relative;
  width: 100%;
  height: 430rpx;
  border-radius: 24rpx;
}
.pic-wrapper image{
  position: relative;
  width: 430rpx;
  height: 430rpx;
}

.price{
  position: relative;
  width: 200rpx;
  height: 200rpx;
  top: 0;
  text-align: center;
  border-radius: 0 0 50% 50%;
  background-color: pink;
}

.bargain{
  position: relative;
  top: 50rpx;
  left: 50%;
  transform: translateX(-50%);
  white-space: nowrap;
  font-size: 19rpx;
  color: rgb(255, 0, 0);
}

.shop-price{
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  text-decoration: line-through;
  font-size: 19rpx;
  color: rgb(76, 76, 76);
}

.act-price{
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 36rpx;
  color: rgb(255, 0, 0);
}

.users{
  position: relative;
  width: 148rpx;
  left: 50%;
  transform: translateX(-50%);
  top: 150rpx;
}

.captain-wrapper{
  position: relative;
  box-sizing: border-box;
  width: 64rpx;
  height: 64rpx;
  float: left;
}

.captain{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 1rpx solid orange;
  overflow: hidden;
}
.captain image{
  position: relative;
  width: 100%;
  height: 100%;
}
.captain-mark{
  position: absolute;
  box-sizing: border-box;
  width: 46rpx;
  height: 22rpx;
  top: -5rpx;
  left: -5rpx;
  border-radius: 22rpx;
  line-height: 22rpx;
  font-size: 14rpx;
  text-align: center;
  background-color: orange;
}

.others{
  position: relative;
  box-sizing: border-box;
  width: 64rpx;
  height: 64rpx;
  line-height: 60rpx;
  float: right;
  border-radius: 50%;
  border: 1rpx dotted orange;
  overflow: hidden;
  text-align: center;
  font-size: 40rpx;
  color: orange;
}
.others image{
  width: 100%;
  height: 100%;
}

.buy{
  position: relative;
  box-sizing: border-box;
  width: 142rpx;
  height: 56rpx;
  top: 234rpx;
  left: 50%;
  transform: translateX(-50%);
  border: 1rpx solid rgb(242, 28, 0);
  border-radius: 10rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 24rpx;
  white-space: nowrap;
  color: rgb(242, 28, 0);
}
.buy text{
  display: inline-block;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
}


.middle{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding: 0 20rpx;
  background-color: white;
}

.invite{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx 50rpx; 
  text-align: center;
}
.invite text{
  position: relative;
  margin-top: 20rpx;
  font-size: 24rpx;
}

.share-btn{
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  margin-bottom: 50rpx;
  border-radius: 15rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  color: white;
  background-color: red;
}

.info-list{
  display: flex;
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  padding: 0 20rpx;
  border-top: 2px solid rgb(220, 220, 220);
  text-align: center;
  line-height: 68rpx;
  font-size: 24rpx;
}

.info-left{
  position: relative;
  height: 100%;
  flex: 0 0 100rpx;
  text-align: left;
}

.info-right{
  position: relative;
  box-sizing: border-box;
  height: 100%;
  flex: 1 1 ;
  padding-left: 100rpx;
  text-align: right;
}

.rule-info {
  display: inline-block;
  position: relative;
  box-sizing: border-box;
  list-style: disc;
}
