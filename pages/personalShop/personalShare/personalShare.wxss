/* pages/personalShop/personalShare/personalShare.wxss */
page{
  position: relative;
  box-sizing: border-box;
  min-height: 100%;
  height: 100%;
  max-height: 100%;
  max-width: 100%;
  padding: 0;
  margin: 0;
  overflow: hidden;
}

view{
  box-sizing: border-box;
}

button::after {
  border: none;
}

button {
  background-color: #fff;
}

.bac{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
}

.share-shop{
  position: relative;
  width: 528rpx;
  height: 912rpx;
  margin-top: 40rpx;
  left: 50%;
  transform: translateX(-50%);
}

.share-goods{
  position: relative;
  width: 580rpx;
  height: 1050rpx;
  margin-top: 40rpx;
  left: 50%;
  transform: translateX(-50%);
}

.btn-wrapper{
  position: fixed;
  width: 100%;
  height: 206rpx;
  bottom: 0;
  padding: 30rpx 176rpx 34rpx 176rpx;
  background-color: white;
  z-index: 10;
}

.btn{
  
  position: relative;
  width: 100rpx;
  height: 100%;
  float: left;
  text-align: center;
}
.btn:last-child{
  float: right;
}
.btn button{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  padding: 0;
  margin: 0;
}
.btn image{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  
}
.btn view{
  position: relative;
  width: 100%;
  height: 24rpx;
  margin-top: 18rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: rgb(86, 86, 86);
}

.shop-canvas-wrapper{
  position: absolute;
  width: 528rpx;
  height: 912rpx;
  left: 4000rpx;

}

.goods-canvas-wrapper{
  position: absolute;
  width: 580rpx;
  height: 1050rpx;
  left: 4000rpx;
}


















