/* pages/personalShop/personanSetting/personalSetting.wxss */
page{
  position: relative;
  box-sizing: border-box;
  min-height: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

view{
  box-sizing: border-box;
}

.wrapper{
  position: relative;
  width: 100%;
  padding: 0 85rpx;
  border-top: 2rpx solid rgb(229, 229, 229);
  text-align: center;
}

.change{
  display: flex;
  position: relative;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 2rpx solid rgb(229, 229, 229);
  text-align: left;
  font-size: 30rpx;
  color: rgb(66, 66, 66);
}

.title{
  position: relative;
  height: 100%;
  flex: 0 0 150rpx;
  margin-right: 30rpx;
}

.middle{
  position: relative;
  height: 100%;
  flex: 1 1 ;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.arrow{
  position: relative;
  height: 28rpx;
  flex: 0 0 14rpx;
  top: 50%;
  transform: translateY(-50%);
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.head{
  position: relative;
  box-sizing: border-box;
  width: 58rpx;
  height: 58rpx;
  top: 50%;
  transform: translateY(-50%);
  float: right;
  border: 4rpx solid rgb(255, 177, 47);
  border-radius: 50%;
}

.change-page{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  padding: 40rpx 20rpx 0 20rpx;
  text-align: left;
  background-color: white;
  z-index: 10;
}

.change-title{
  position: relative;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 40rpx;
  color: rgb(66, 66, 66);
}

.change-sec-title{
  position: relative;
  height: 24rpx;
  margin-top: 20rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: rgb(120, 120, 120);
}

#change-name{
  position: relative;
  width: 100%;
  margin-top: 20rpx;
  font-size: 30rpx;
  border-bottom: 2rpx solid rgb(229, 229, 229);
}

.btn-submit{
  position: relative;
  width: 250rpx;
  height: 50rpx;
  margin-top: 20rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  text-align: center;
}






