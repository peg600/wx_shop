/* pages/lottery/lottery.wxss */
@import "lottery_header.wxss";

page {
  margin-bottom: 25rpx;
}

view {
  box-sizing: border-box;
}

.intro_page {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 100;
  text-align: center;
  overflow: hidden;
  background: linear-gradient(#fc3e24, #fdebbc);
}

.main_entry {
  z-index: 101;
  position: absolute;
  top: 910rpx;
  left: 0rpx;
  width: 666rpx;
}

.intro_page image {
  width: 750rpx;
}

.navigator_btn {
  position: relative;
  width: 700rpx;
  margin-top: 20rpx;
  margin-left: 25rpx;
}

.navigator_btn image {
  width: 700rpx;
}

.address_info_container {
  position: relative;
  margin-top: 310rpx;
  width: 100%;
  height: 800rpx;
  background: url(https://wao-files.oss-cn-beijing.aliyuncs.com/holo/address_bg_1.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  text-align: center;
  align-items: center;
  padding: 70rpx 58rpx;
}

.address_info_container checkbox-group {
  margin-top: 40rpx;
  text-align: left;
  font-size: 30rpx;
  color: #e63117;
}

.address_info_container checkbox-group checkbox {
  display: inline-block;
  font-size: 25rpx;
  height: 30rpx;
}

.rule_link {
  display: inline-block;
}

.address_title {
  font-size: 34rpx;
  font-weight: bold;
  color: #e63117;
  margin-bottom: 20rpx;
  text-align: center;
}

.address {
  border: 1rpx solid #d7d8e0;
  border-radius: 10rpx;
  margin-top: 32rpx;
  padding: 15rpx 22rpx;
  text-align: left;
  font-size: 30rpx;
  height: 76rpx;
}

.from {
  width: 442rpx;
  display: inline-block;
}

.from_container {
  margin-top: 40rpx;
  display: block;
}

.location {
  display: inline-block;
  width: 168rpx;
  position: relative;
  height: 76rpx;
  margin-left: 24rpx;
  vertical-align: middle;
  font-size: 30rpx;
  color: white;
  font-weight: bold;
  margin-top: -5rpx;
}

.location image {
  width: 168rpx;
  height: 76rpx;
}

.address text {
  display: inline-block;
  color: #888;
}

.noborder {
  margin-left: 20rpx;
  display: inline-block;
  font-size: 30rpx;
  font-weight: bold;
}

.main_info_wrapper {
  position: relative;
  margin-top: 310rpx;
  width: 100%;
  height: 353rpx;
  background: url(https://wao-files.oss-cn-beijing.aliyuncs.com/holo/address_bg_no_button.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  text-align: center;
  align-items: center;
}

.btn_circle_get_ticket {
  width: 284rpx;
  height: 284rpx;
}

.main_info_wrapper .today {
  position: absolute;
  left: 195rpx;
  right: 50rpx;
  text-align: left;
  font-size: 32rpx;
  color: #be6b0e;
  top: 85rpx;
}

.main_info_wrapper .today .info {
  font-weight: bold;
  font-size: 35rpx;
}

.btn_submit {
  margin-top: 50rpx;
  border-radius: 40rpx;
  height: 80rpx;
  text-align: center;
  padding-top: 15rpx;
  vertical-align: middle;
  font-size: 40rpx;
  font-weight: bold;
  color: #762400;
  background: #ffd521;
}

.main_info_wrapper .to_end {
  position: absolute;
  right: 5rpx;
  width: 260rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #be6b0e;
  top: 85rpx;
}

.from_to_info {
  background: red;
  border-radius: 10rpx;
  background: linear-gradient(to right, #fc3e24, #fe5935);
  position: absolute;
  left: 50rpx;
  top: 152rpx;
  height: 154rpx;
  width: 650rpx;
}

.from_to_info .text {
  font-size: 36rpx;
  color: #fff;
  white-space: nowrap;
  overflow-x: hidden;
  width: 220rpx;
  overflow: hidden;
  text-align: center;
}

.from_to_info .info {
  font-size: 28rpx;
  color: #fff;
}

.from_to_info image {
  width: 50%;
  margin-top: 32rpx;
}

.from_text {
  position: absolute;
  left: 5rpx;
  top: 32rpx;
  font-weight: bold;
}

.from_info {
  position: absolute;
  left: 5rpx;
  top: 94rpx;
}

.to_text {
  position: absolute;
  right: 5rpx;
  top: 32rpx;
  font-weight: bold;
}

.to_info {
  position: absolute;
  right: 5rpx;
  top: 94rpx;
}

.title {
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 40rpx;
  color: black;
}

.rule {
  position: absolute;
  width: 120rpx;
  height: 60rpx;
  top: 20rpx;
  left: 20rpx;
  border: 1rpx solid #ddd;
  line-height: 58rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: black;
}

.back {
  position: absolute;
  width: 120rpx;
  height: 60rpx;
  top: 20rpx;
  right: 20rpx;
  border: 1rpx solid #ddd;
  line-height: 58rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: black;
}

.lottery-wrapper {
  position: relative;
  width: 100%;
  padding: 50rpx 0;
  font-size: 50rpx;
  color: black;
}

.lottery-info {
  position: relative;
  width: 100%;
  padding: 0 30rpx;
}

.lottery-title {
  position: relative;
  margin-bottom: 70rpx;
}

.my-lottery {
  display: inline-block;
  position: absolute;
  height: 80rpx;
  top: 0;
  right: 50rpx;
  line-height: 80rpx;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
}

.lottery {
  display: flex;
  position: relative;
  width: 100%;
}

.lottery-left {
  position: relative;
  flex: 1 1;
  height: 100%;
}

.lottery-right {
  position: relative;
  flex: 0 0 300rpx;
  height: 300rpx;
  line-height: 300rpx;
  border: 1rpx solid #ddd;
  border-radius: 50%;
  text-align: center;
  font-size: 40rpx;
}

.rule-area {
  position: relative;
  width: 100%;
}

.before-submit-wrapper {
  position: relative;
  width: 100%;
  font-size: 30rpx;
}

.user_image {
  position: absolute;
  left: 50rpx;
  top: 25rpx;
  width: 110rpx;
  height: 110rpx;
  box-sizing: border-box;
  overflow: hidden;
  border-radius: 50% 50%;
}

.user_image .head_image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.user_welcome {
  position: absolute;
  left: 195rpx;
  right: 50rpx;
  top: 35rpx;
  color: #be6b0e;
  font-size: 32rpx;
  text-align: left;
}

.userNickName {
  font-size: 35rpx;
  font-weight: bold;
}

.navigator_btn_image {
  border-radius: 10rpx;
  box-shadow: 0px 5rpx 35rpx 0rpx rgba(187, 127, 16, 0.52);
  text-shadow: 0px 5rpx 35rpx 0rpx rgba(187, 127, 16, 0.52);
}

.bottombutton {
  position: absolute;
  width: 208rpx;
  height: 300rpx;
  font-size: 40rpx;
  bottom: -115rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10;
}

.leftbutton {
  left: 72rpx;
}

.rightbutton {
  right: 72rpx;
}

.bottombutton image {
  width: 208rpx;
  height: 33rpx;
  transform: translateY(-50%);
}

.inner1 .title {
  font-weight: bold;
  font-size: 40rpx;
  color: #e63117;
  margin-bottom: 20rpx;
}

.inner1 .hasMargin {
  margin-top: 20rpx;
}

.inner1 .hasBottomMargin {
  margin-bottom: 20rpx;
}

.inner1 .hasLeftMargin {
  margin-left: 30rpx;
}

.inner1 .listitem {
  margin-bottom: 20rpx;
  margin-left: 15rpx;
}

.inner1 .listitem image
{
  width: 40rpx;
}

.inner1 .index {
  font-weight: bold;
  color: #e63117;
}

.numberIndex {
  color: #e63117;
  font-weight: bold;
}

.inner1 {
  overflow-y: auto;
  height: 750rpx;
}

.noaddress_tip
{
  position: absolute;
  left: 15rpx;
  width: 620rpx;
  text-align: center;
  bottom: 10rpx;
  font-size: 30rpx;
  color: white;
}

.submitAddress
{
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  width: 150rpx;
  margin-left: 20rpx;
  color: #be6b0e;
  background: #ffd521;
  display: inline-block;
  font-weight: bold;
}

.closeButton
{
  width:100rpx;
  height: 100rpx;
  position: absolute;
  top: 10rpx;
  right: 30rpx;
  display: flex;
  justify-content: center;
  text-align: center;
  align-items: center;
}

.closeButton image
{
  width: 50rpx;
  height: 50rpx;
}