.ticket_container {
  padding-top: 50rpx;
  width: 100%;
  position: relative;
}

.title {
  position: fixed;
  top: 0rpx;
  padding-top: 50rpx;
  display: flex;
  flex-direction: row;
  width: 100%;
  font-size: 36rpx;
  font-weight: bold;
  border-bottom: 1rpx solid #f5f5f5;
  background: white;
  z-index: 1000;
}

.list
{
  padding:25rpx;
}

.head {
  flex: 1;
  text-align: center;
  padding: 10rpx 10rpx;
}

.selected {
  border-bottom: 5rpx solid #ec3721;
}

.content {
  margin-top: 125rpx;
}

.list-item {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}

.stamp {
  width: 420rpx;
  height: 150rpx;
  padding: 0rpx 25rpx;
  position: relative;
  overflow: hidden;
  margin-bottom: 20rpx;
  color: white;
}

.stamp:before {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 10rpx;
  right: 10rpx;
  z-index: -1;
}

.stamp:after {
  content: '';
  position: absolute;
  left: 10rpx;
  top: 10rpx;
  right: 10rpx;
  bottom: 10rpx;
  box-shadow: 0 0 20rpx 1rpx rgba(0, 0, 0, 0.5);
  z-index: -2;
}

.stamp01 {
  background: #ec3721;
  background: radial-gradient(rgba(0, 0, 0, 0) 0, rgba(0, 0, 0, 0) 5rpx, #ec3721 5rpx);
  background-size: 15rpx 15rpx;
  background-position: 9rpx 3rpx;
}

.stamp01:before {
  background-color: #ec3721;
}

.stamp .mainTitle{
  font-size: 36rpx;
  display: inline-block;
}

.stamp .sub
{
  font-size: 30rpx;
  display: inline-block;
  margin-left: 25rpx;
  text-align: center;
}

.stamp .top
{
  padding:25rpx 10rpx 25rpx 0;
}

.stamp .bottomInfo{
  font-size: 26rpx;
  height: 50rpx;
}

.info
{
  font-size: 30rpx;
}

.lottery_info
{
  padding:25rpx;
}

.all_info_title
{
  padding:25rpx;
}

.but_other{
  margin-top: 24rpx;
  background: #fff
}
.but_other .but_text{
  margin-left: 25rpx;
  padding: 25rpx 0;
  margin-bottom: 20rpx;
  padding-top: 25rpx;
  font-size: 36rpx;
  font-weight: bold;
}