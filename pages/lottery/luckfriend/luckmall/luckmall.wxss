.mall_header
{
  height: 150rpx;
  line-height: 150rpx;
  font-size: 50rpx;
  display: flex;
  justify-content: left;
  align-items: center;
  padding-left: 30rpx;
}

.mall_header image{
  width: 80rpx;
  margin-left: 25rpx;
}

