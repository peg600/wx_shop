.tit{
  font-size: 48rpx;
  color: rgb(53, 53, 53); 
  margin-top: 60rpx;
  margin-bottom: 58rpx;
  margin-left: 50rpx;
}
.ipt-box{
  width: 690rpx;
  margin: 0 auto;
}
.ipt-box input{
  font-size: 30rpx;
  color: rgb(136, 136, 136);
  padding: 30rpx 0 30rpx 20rpx;
  border-bottom: 1px solid rgb(230, 230, 230);
}
.ma{
  position: relative;
}
.ma button{
  position: absolute;
  width:210rpx;
  height: 70rpx;
  background: rgb(255, 251, 229);
  font-size: 24rpx;
  color: rgb(136, 136, 136);
  text-align: center;
  top: 20rpx;
  right: 0;
  border-radius: 45rpx;
  border: 0;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10;
}
.confirm button{
  width: 690rpx;
  margin: 90rpx auto 0;
  font-size: 34rpx;
  color: rgb(255, 255, 255);
  height: 90rpx;
  background: rgb(255, 211, 2);
  border-radius: 45rpx;
  border: 0;
}
.rd{
  margin-top: 35rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: rgb(136, 136, 136);
}
.rd image{
  width: 30rpx;
  height: 30rpx;
  /* background: #ccc; */
  border-radius: 50%;
  margin-right: 8rpx;
}
.rd .sm{
  color: rgb(254, 192, 0)
}
.t-img{
  height: 360rpx;
  padding: 150rpx 0;
  text-align: center;
  box-sizing: border-box;
}
.t-img image{
  width: 120rpx;
  height: auto;
  /* height: 235rpx; */
}
page{
  height: 100%;
}
.yb-box{
  height: 100%;
  background: #f3f2f8;
  position: relative;
}
.yb,.tm{
  text-align: center;
  font-size: 36rpx;
}
.yb{
  color: #848484;
}
.qr {
  position: absolute;
  bottom: 142rpx;
  width: 700rpx;
  height: 90rpx;
  box-sizing: border-box;
  left: 25rpx;
  text-align: center;
}
.qr button{
  width: 100%;
  height: 100%;
  border: 1px solid #febf00;
  background: #febf00;
  color: rgb(49,49,49);
  font-size: 34rpx;
  /* padding: 25rpx 0; */
  border-radius: 10rpx;
}
.jb{
  position: absolute;
  bottom: 26rpx;
  width: 700rpx;
  height: 90rpx;
  box-sizing: border-box;
  left: 25rpx;
  text-align: center;
}
.jb button{
  width: 100%;
  height: 100%;
  border: 1px solid #ccc;
  color: rgb(49,49,49);
  font-size: 34rpx;
  /* padding: 25rpx 0; */
  border-radius: 10rpx;
  background: #fff;
}
.img-box{
  width: 30rpx;
  height: 30rpx;
  /* background: #ccc; */
  border-radius: 50%;
  margin-right: 5rpx;
}
.select{
    border: 1px solid #888888;
}
